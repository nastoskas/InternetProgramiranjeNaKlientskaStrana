<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
<script type="text/javascript">
    let year = prompt("Vnesete godina: ");
    year = parseInt(year);
    if(!isNaN(year))
    {
        if((year%4==0 && year%100!=0)||year%400==0)
        {
            document.write("Godinata e prestapna");
        }
        else{
            document.write("Godinata ne e prestapna");
        }
    }
    else{
        document.write("Vnesete celi broevi!");
    }
</script>
</body>
</html>
